/**index.wxss**/
page {
  padding-top: 260rpx;
  box-sizing: border-box;
}
.title {
  margin-bottom: 160rpx;
  font-size: 42rpx;
  text-align: center;
  font-weight: bold;
}
.ul {
  margin: 0 auto;
  width: 400rpx;
  font-size: 32rpx;
}
.ul .li {
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
}
.ul .li:last-child {
  margin-bottom: 0;
}
.ul .li .num {
  display: inline-block;
  margin-right: 20rpx;
  width: 34rpx;
  line-height: 34rpx;
  background-color: #333;
  font-size: 28rpx;
  color: #fff;
  border-radius: 50%;
  text-align: center;
}
.upload-btn {
  margin-top: 200rpx;
  width: 460rpx;
}
.bg-img {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 100%;
}